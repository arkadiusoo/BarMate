	<modules>
		<module>inventory-service</module>
		<module>shopping_service</module>
		<module>analytics-service</module>
        <module>user-service</module>
		<module>AppRunner</module>
		<module>cocktail-service</module>
		<module>Web</module>
		<module>cocktail-service</module>
